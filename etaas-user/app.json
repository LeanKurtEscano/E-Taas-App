{
  "expo": {
    "name": "ETAAS",
    "slug": "etaas-user",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "etaasuser",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.etaasuser",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "ETAAS needs access to your location to help you set your delivery address and show nearby laundry shops.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "ETAAS needs access to your location to help you set your delivery address and show nearby laundry shops."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "package": "com.dti.etaasuser",
      "versionCode": 2
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/etaas.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ec4899",
          "dark": {
            "backgroundColor": "#ec4899"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow ETAAS to use your location to show nearby laundry shops and provide delivery services.",
          "locationWhenInUsePermission": "Allow ETAAS to use your location to show nearby laundry shops and provide delivery services.",
          "isAndroidBackgroundLocationEnabled": false,
          "isIosBackgroundLocationEnabled": false
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "824b4320-2015-4360-947d-0a8fafaa6298"
      }
    }
  }
}
